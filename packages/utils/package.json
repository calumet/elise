      "require": "./dist/index.cjs"
    },
    "./forms": {
      "types": "./dist/forms/index.d.ts",
      "import": "./dist/forms/index.mjs",
      "require": "./dist/forms/index.cjs"
    },
    "./toasts": {
      "types": "./dist/toasts/index.d.ts",
      "import": "./dist/toasts/index.mjs",
      "require": "./dist/toasts/index.cjs"
    },
    "./alerts": {
      "types": "./dist/alerts/index.d.ts",
      "import": "./dist/alerts/index.mjs",
      "require": "./dist/alerts/index.cjs"
    },
    "./dates": {
      "types": "./dist/dates/index.d.ts",
      "import": "./dist/dates/index.mjs",
      "require": "./dist/dates/index.cjs"
    },
    "./tables": {
      "types": "./dist/tables/index.d.ts",
      "import": "./dist/tables/index.mjs",
      "require": "./dist/tables/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --dts",
    "dev": "tsup --watch --dts",
    "lint": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@calumet/elise-icons": "workspace:*",
    "@calumet/elise-ui": "workspace:*",
