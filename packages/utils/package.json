{
  "name": "@calumet/elise-utils",
  "version": "0.1.1",
  "private": false,
  "description": "Utilidades compartidas para Elise (validaciÃ³n, helpers).",
  "repository": {
    "type": "git",
    "url": "https://github.com/calumet/elise.git",
    "directory": "packages/utils"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
