    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.4",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@calumet/elise-ui": "workspace:*",
    "react": "^19.0.0",
